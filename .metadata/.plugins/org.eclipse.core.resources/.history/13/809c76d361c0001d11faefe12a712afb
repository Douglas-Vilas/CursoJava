package sintaxe_variavel_e_fluxo;

public class TestaVariavel {

	public static void main(String[] args) {
		
		int idade;
		idade = 32;
		
		System.out.println(idade);
		
		idade = 30 + 10;
		
		System.out.println(idade);
		
		idade = (7 * 5) + 2;
		
		System.out.println(idade);
		
		System.out.println("A idade Ã©" + idade);

	}

}
